{
    "alt-speed-down": 50,
    "alt-speed-enabled": false,
    "alt-speed-time-begin": 540,
    "alt-speed-time-day": 127,
    "alt-speed-time-enabled": false,
    "alt-speed-time-end": 1020,
    "alt-speed-up": 50,
    "bind-address-ipv4": "<%= scope['::transmission::bind_address_ipv4'] %>",
    "bind-address-ipv6": "<%= scope['::transmission::bind_address_ipv6'] %>",
<% if scope['::transmission::blocklist_url'] -%>
    "blocklist-enabled": true,
    "blocklist-url": "<%= scope['::transmission::blocklist_url'] %>",
<% else -%>
    "blocklist-enabled": false,
    "blocklist-url": "http://www.example.com/blocklist",
<% end -%>
    "cache-size-mb": 4,
    "dht-enabled": <%= scope['::transmission::dht_enabled'] %>,
<% if scope['::transmission::download_dir'] =~ /^\// -%>
    "download-dir": "<%= scope['::transmission::download_dir'] %>",
<% else -%>
    "download-dir": "<%= scope['::transmission::params::home_dir'] %>/<%= scope['::transmission::download_dir'] %>",
<% end -%>
    "download-limit": 100,
    "download-limit-enabled": 0,
<% if scope['::transmission::download_queue_size'] -%>
    "download-queue-enabled": true,
    "download-queue-size": <%= scope['::transmission::download_queue_size'] %>,
<% else -%>
    "download-queue-enabled": false,
    "download-queue-size": 5,
<% end -%>
    "encryption": <%= scope['::transmission::encryption'] %>,
    "idle-seeding-limit": 30,
    "idle-seeding-limit-enabled": false,
<% if scope['::transmission::incomplete_dir'] -%>
<% if scope['::transmission::incomplete_dir'] =~ /^\// -%>
    "incomplete-dir": "<%= scope['::transmission::incomplete_dir'] %>",
<% else -%>
    "incomplete-dir": "<%= scope['::transmission::params::home_dir'] %>/<%= scope['::transmission::incomplete_dir'] %>",
<% end -%>
    "incomplete-dir-enabled": true,
<% else -%>
<% if scope['::transmission::incomplete_dir'] =~ /^\// -%>
    "incomplete-dir": "<%= scope['::transmission::incomplete_dir'] %>",
<% else -%>
    "incomplete-dir": "<%= scope['::transmission::params::home_dir'] %>/<%= scope['::transmission::incomplete_dir'] %>",
<% end -%>
    "incomplete-dir-enabled": false,
<% end -%>
    "lpd-enabled": false,
    "max-peers-global": 200,
    "message-level": <%= scope['::transmission::message_level'] %>,
    "peer-congestion-algorithm": "",
    "peer-id-ttl-hours": 6,
    "peer-limit-global": 200,
    "peer-limit-per-torrent": 50,
    "peer-port": <%= scope['::transmission::peer_port'] %>,
    "peer-port-random-high": <%= scope['::transmission::peer_port_random_high'] %>,
    "peer-port-random-low": <%= scope['::transmission::peer_port_random_low'] %>,
    "peer-port-random-on-start": false,
    "peer-socket-tos": "default",
    "pex-enabled": <%= scope['::transmission::pex_enabled'] %>,
    "port-forwarding-enabled": <%= scope['::transmission::port_forwarding_enabled'] %>,
    "preallocation": <%= scope['::transmission::preallocation'] %>,
    "prefetch-enabled": 1,
    "queue-stalled-enabled": true,
    "queue-stalled-minutes": 30,
<% if scope['::transmission::ratio_limit'] -%>
    "ratio-limit": <%= scope['::transmission::ratio_limit'] %>,
    "ratio-limit-enabled": true,
<% else -%>
    "ratio-limit": 2,
    "ratio-limit-enabled": false,
<% end -%>
    "rename-partial-files": true,
    "rpc-authentication-required": <%= scope['::transmission::rpc_enable_auth'] %>,
    "rpc-bind-address": "<%= scope['::transmission::params::rpc_bind'] %>",
    "rpc-enabled": true,
<% if scope['::transmission::rpc_password'] -%>
    "rpc-password": "<%= scope['::transmission::rpc_password'] %>",
<% else -%>
    "rpc-password": "{934145379c329831675007802bf4e43dc0ebd677BCqMicmZ",
<% end -%>
    "rpc-port": <%= scope['::transmission::rpc_port'] %>,
    "rpc-url": "<%= scope['::transmission::rpc_base_url'] %>",
    "rpc-username": "<%= scope['::transmission::rpc_username'] %>",
<% if scope['::transmission::rpc_whitelist'] -%>
    "rpc-whitelist": "127.0.0.1,<%= scope['::transmission::rpc_whitelist'] %>",
    "rpc-whitelist-enabled": true,
<% else -%>
    "rpc-whitelist": "127.0.0.1",
    "rpc-whitelist-enabled": false,
<% end -%>
    "scrape-paused-torrents-enabled": true,
<% if scope['::transmission::script_torrent_done_filename'] -%>
    "script-torrent-done-enabled": true,
    "script-torrent-done-filename": "<%= scope['::transmission::script_torrent_done_filename'] %>",
<% else -%>
    "script-torrent-done-enabled": false,
    "script-torrent-done-filename": "",
<% end -%>
    "seed-queue-enabled": false,
    "seed-queue-size": 10,
<% if scope['::transmission::speed_limit_down'] -%>
    "speed-limit-down": <%= scope['::transmission::speed_limit_down'] %>,
    "speed-limit-down-enabled": true,
<% else -%>
    "speed-limit-down": 100,
    "speed-limit-down-enabled": false,
<% end -%>
<% if scope['::transmission::speed_limit_up'] -%>
    "speed-limit-up": <%= scope['::transmission::speed_limit_up'] %>,
    "speed-limit-up-enabled": true,
<% else -%>
    "speed-limit-up": 100,
    "speed-limit-up-enabled": false,
<% end -%>
    "start-added-torrents": true,
    "trash-original-torrent-files": false,
<% if scope['::transmission::umask'] -%>
    "umask": <%= scope['::transmission::umask'] %>,
<% else -%>
    "umask": 18,
<% end -%>
    "upload-limit": 100,
    "upload-limit-enabled": 0,
    "upload-slots-per-torrent": 14,
    "utp-enabled": <%= scope['::transmission::utp_enabled'] %>,
<% if scope['::transmission::watch_dir'] -%>
<% if scope['::transmission::watch_dir'] =~ /^\// -%>
    "watch-dir": "<%= scope['::transmission::watch_dir'] %>",
<% else -%>
    "watch-dir": "<%= scope['::transmission::params::home_dir'] %>/<%= scope['::transmission::watch_dir'] %>",
<% end -%>
    "watch-dir-enabled": true
<% else -%>
    "watch-dir-enabled": false
<% end -%>
}
